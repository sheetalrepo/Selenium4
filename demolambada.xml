<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="3" name="ProdBug" parallel="tests">

	<test name="Chrome86WinTest">
		<parameter name="browser" value="Chrome"/>
		<parameter name="version" value="86.0"/>
		<parameter name="platform" value="Windows 10"/>
		<classes>
			<class name="com.lambdaTest.DemoSeleniumClassGrid"/>
		</classes>
	</test>

	<test name="Edge87MacTest">
		<parameter name="browser" value="MicrosoftEdge"/>
		<parameter name="version" value="87.0"/>
		<parameter name="platform" value="macOS Sierra"/>
		<classes>
			<class name="com.lambdaTest.DemoSeleniumClassGrid"/>
		</classes>
	</test>

</suite>